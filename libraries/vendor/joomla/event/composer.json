{
    "name": "joomla/event",
    "type": "joomla-package",
    "description": "Joomla Event Package",
    "keywords": ["joomla", "framework", "event"],
    "homepage": "https://github.com/joomla-framework/event",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10|>=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|~5.0",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Event\\": "src/",
            "Joomla\\Event\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
